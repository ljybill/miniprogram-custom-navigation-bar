@font-face {
  font-family: 'ionicons';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}
.icon {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'ionicons' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-arrow-back:before {
  content: "\e90d";
}
.icon-home:before {
  content: "\eb19";
}
.nav {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 20;
  background: #fff;
}
.nav-content {
  box-sizing: content-box;
  position: relative;
  height: 32px;
}
.nav-icon {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  box-sizing: border-box;
  width: 200rpx;
  display: flex;
  align-items: center;
}
.nav-icon .icon {
  height: 100%;
  font-size: 28px;
  padding: 0 2px;
  margin-left: 10rpx;
  flex: none;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-title {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 200rpx;
  box-sizing: border-box;
}
.nav-title-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.navigator-hover {
  background-color: rgba(0, 0, 0, 0.1);
  opacity: 0.7;
}
